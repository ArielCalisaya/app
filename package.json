{
  "name": "app",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://github.com/breatheco-de/app",
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "npm run test && next build",
    "start": "next start -p 3080",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "jest": "jest",
    "storybook": "npm run make:iconDict && start-storybook -p 6006",
    "build-storybook": "npm run make:iconDict && build-storybook",
    "make:iconDict": "node ./scripts/make.iconDict.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "node --experimental-modules ./src/__tests__/publicSyllabus.js",
    "lint:files": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "lint:fix:components": "eslint --fix src/common/components/**/*.{js,jsx}",
    "prepare": "husky install",
    "make:sitemap": "node ./scripts/sitemap-generator.js",
    "make:syllabus": "node ./scripts/syllabus.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.15",
    "@chakra-ui/react": "1.6.5",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@fontsource/lato": "4.5.0",
    "@loadable/component": "5.15.0",
    "@next/bundle-analyzer": "11.0.1",
    "atob": "2.1.2",
    "axios": "0.24.0",
    "date-fns": "2.28.0",
    "emoji-dictionary": "^1.0.11",
    "express": "^4.18.1",
    "formik": "2.2.9",
    "framer-motion": "4.1.17",
    "globby": "^11.0.1",
    "gray-matter": "4.0.3",
    "history": "5.0.1",
    "markdown-to-jsx": "7.1.3",
    "next": "^12.1.0",
    "next-redux-wrapper": "7.0.2",
    "next-runtime-dotenv": "1.4.0",
    "next-translate": "^1.3.5",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-confetti": "^6.1.0",
    "react-cookie": "^4.1.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "17.0.2",
    "react-easy-crop": "^4.5.0",
    "react-gtm-module": "^2.0.11",
    "react-player": "^2.10.1",
    "react-redux": "7.2.4",
    "react-select": "^5.4.0",
    "react-syntax-highlighter": "15.4.5",
    "react-youtube": "^7.14.0",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.3.0",
    "refractor": "^4.5.0",
    "remark": "14.0.1",
    "remark-comments": "1.2.9",
    "remark-emoji": "3.0.2",
    "remark-html": "15.0.0",
    "remark-react": "9.0.1",
    "remark-toc": "8.0.1",
    "remark-variables": "1.4.9",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "5.3.0",
    "swr": "^1.3.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@next/eslint-plugin-next": "^12.0.8",
    "@storybook/addon-actions": "6.3.7",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/addon-knobs": "6.3.0",
    "@storybook/addon-links": "6.3.6",
    "@storybook/react": "6.4.19",
    "@testing-library/react": "12.0.0",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.13.2",
    "cypress": "8.3.0",
    "cypress-terminal-report": "3.3.2",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "7.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.1"
  }
}
